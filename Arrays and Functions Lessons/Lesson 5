#Modify
def add(a, b):
  return a + b
try:
  
  a = float(input("Please Input A Value For A:"))
  b = float(input("Please Input A Value For B:"))
  print(add(a, b)*2)
  
except:
  print("Invalid Number Try Again:")

#Modify 2
import math 
def myPow(m, n):
  return math.pow(m, n)
  
print(myPow(7, 2))
