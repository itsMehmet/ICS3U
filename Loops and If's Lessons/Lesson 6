#Try This
S = "#"
for i in range(10, 0, -1):
    print(S * i)


#Modify 2:

S = "#"
Space_Count = 4
for i in range(1, 10, 2):
    print(" " * Space_Count, S * i)
    Space_Count -= 1

#Modify 3
S = "#"
Space_Count = 5
for i in range(1, 12, 2):
    print(" " * Space_Count, S * i)
    Space_Count -= 1
Space_Count = 1
for i in range(9, 0, -2):
    print(" " * Space_Count, S * i)
    Space_Count += 1
